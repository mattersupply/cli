{
  "name": "@mattersupply/cli",
  "description": "Matter Supply CLI",
  "version": "1.0.1-pre.18",
  "author": "@mattersupply.co",
  "bin": {
    "matter": "./bin/run"
  },
  "bugs": "https://github.com/mattersupply/cli/issues",
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.345.0",
    "@aws-sdk/client-ssm": "^3.421.0",
    "@aws-sdk/credential-providers": "^3.345.0",
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.4.7",
    "@types/findup-sync": "^2.0.2",
    "axios": "^1.4.0",
    "chalk": "^4.1.2",
    "findup-sync": "^4.0.0",
    "js-yaml": "^4",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.31",
    "@types/chai": "^4",
    "@types/js-yaml": "^4.0.5",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.39",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "chai": "^4",
    "eslint": "8.46.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^13.4.12",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "eslint-config-prettier": "^8.10.0",
    "mocha": "^9",
    "nock": "^13.3.2",
    "oclif": "^3",
    "shx": "^0.3.3",
    "sinon": "^15.2.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.1",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/mattersupply/cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "topics": {
      "config": {
        "description": "Manage remote configuration & secret values"
      }
    },
    "commands": "./lib/commands",
    "bin": "matter",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "mattersupply/cli",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "update-readme": "oclif readme",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
